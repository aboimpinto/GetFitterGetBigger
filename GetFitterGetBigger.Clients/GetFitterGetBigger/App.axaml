<Application 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GetFitterGetBigger"
    xmlns:navigationManager="using:Olimpo.NavigationManager"
    x:Class="GetFitterGetBigger.App"
    RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <navigationManager:ViewLocator/>
    </Application.DataTemplates>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://Olimpo.Controls/Assets/IconResources.axaml"/>
                <ResourceInclude Source="/Assets/GetFitterGetBiggerResources.axaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <FluentTheme />

        <!-- <StyleInclude Source="avares://CherylUI/Index.axaml" /> -->

        <StyleInclude Source="avares://Olimpo.Controls/Assets/Olimpo.Controls.Styles.axaml"/>
        <StyleInclude Source="/Assets/CommonStyles.axaml"/>
    </Application.Styles>
</Application>
